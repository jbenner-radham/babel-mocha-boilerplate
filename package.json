{
  "name": "babel-mocha-boilerplate",
  "version": "0.0.0",
  "private": true,
  "description": "A project boilerplate for Babel and Mocha.",
  "keywords": [],
  "license": "MIT",
  "author": "James Benner <james.benner@gmail.com> (http://www.jamesbenner.com/)",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "babel --out-dir dist lib",
    "lint": "eslint {lib,test}",
    "lint:fix": "eslint --fix {lib,test}",
    "prepare": "husky install",
    "sort:pkg": "sort-package-json",
    "test": "mocha",
    "test:watch": "mocha --watch"
  },
  "dependencies": {
    "core-js": "^3.19.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/register": "^7.16.0",
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "husky": "^7.0.0",
    "mocha": "^9.1.3",
    "sort-package-json": "^1.53.1"
  }
}
